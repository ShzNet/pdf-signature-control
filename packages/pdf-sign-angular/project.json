{
  "name": "@shznet/pdf-sign-angular",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/pdf-sign-angular/src",
  "prefix": "lib",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/angular:package",
      "outputs": [
        "{workspaceRoot}/dist/{projectRoot}"
      ],
      "options": {
        "project": "packages/pdf-sign-angular/ng-package.json",
        "tsConfig": "packages/pdf-sign-angular/tsconfig.lib.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "packages/pdf-sign-angular/tsconfig.lib.prod.json"
        },
        "development": {}
      },
      "defaultConfiguration": "production"
    },
    "nx-release-publish": {
      "options": {
        "packageRoot": "dist/{projectRoot}"
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": [
        "{projectRoot}/test-output/jest/coverage"
      ],
      "options": {
        "jestConfig": "packages/pdf-sign-angular/jest.config.cts",
        "tsConfig": "packages/pdf-sign-angular/tsconfig.spec.json"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    },
    "publish": {
      "command": "npm publish",
      "options": {
        "cwd": "dist/packages/pdf-sign-angular"
      },
      "dependsOn": [
        "build"
      ]
    }
  }
}